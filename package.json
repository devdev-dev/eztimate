{
  "name": "eztimate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run codegen",
    "dev": "next dev",
    "prebuild": "npm run codegen",
    "build": "next build",
    "start": "next start",
    "eslint": "npx eslint \"**/*.{ts,tsx}\"",
