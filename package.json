{
  "name": "eztimate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run clean:gql && npm run codegen",
    "dev": "next dev",
    "prebuild": "npm run clean:gql && npm run codegen",
    "build": "next build",
    "start": "next start",
    "prettier": "npx prettier --check \"**/*.{ts,tsx,css,json,js,jsx}\"",
    "prettier:write": "npx prettier --write \"**/*.{ts,tsx,json}\"",
    "lint": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "npx eslint --fix \"**/*.{ts,tsx}\"",
    "clean:gql": "rm -rf ./.next/.cache/next-babel-loader",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "@harelpls/use-pusher": "^7.2.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@sentry/browser": "^6.2.5",
    "@sentry/integrations": "^6.2.5",
    "@sentry/node": "^6.2.5",
    "@sentry/types": "^6.2.5",
    "@sentry/webpack-plugin": "^1.14.2",
    "apollo-server-micro": "^2.22.2",
    "cookies": "^0.8.0",
    "graphql": "^15.5.0",
    "js-cookie": "^2.2.1",
    "mongodb": "^3.6.5",
    "next": "^10.1.3",
    "next-auth": "^3.13.3",
    "pusher": "^5.0.0",
    "react": "17.0.2",
    "react-avatar-editor": "^11.1.0",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-codegen/typescript-react-apollo": "^2.2.3",
    "@types/cookies": "^0.7.6",
    "@types/graphql": "^14.5.0",
    "@types/js-cookie": "^2.2.6",
    "@types/mongodb": "^3.6.11",
    "@types/next-auth": "^3.13.0",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "prettier": "2.2.1",
    "typescript": "^4.2.3"
  }
}
