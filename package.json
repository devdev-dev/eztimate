    "@emotion/styled": "^11.3.0",
    "@harelpls/use-pusher": "^7.2.1",
    "@material-ui/core": "^5.0.0-beta.1",
    "@material-ui/icons": "^5.0.0-beta.1",
    "@material-ui/system": "^5.0.0-beta.1",
    "apollo-server-micro": "^3.1.2",
    "cookies": "^0.8.0",
    "graphql": "^15.5.1",
    "js-cookie": "^3.0.0",
    "mongodb": "^4.1.1",
